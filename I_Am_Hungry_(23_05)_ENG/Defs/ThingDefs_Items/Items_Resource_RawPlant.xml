<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ########## Resources ########## -->
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>IAmHungry_RawWheat</defName> <!-- Patch: Removed when RimCusine 1 or 2 is found ### Check: VGP -->
    <label>wheat</label>
    <soundInteract>Grain_Drop</soundInteract>
    <soundDrop>Grain_Drop</soundDrop>
    <description>Raw wheat.</description>
    <graphicData>
      <texPath>Things/Resource/Wheat</texPath>
    </graphicData>
    <statBases>
      <MarketValue>1.1</MarketValue>
    </statBases>
    <ingestible>
      <foodType>Plant</foodType> <!--  -->
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>30</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
</ThingDef>

<!-- Patch: Removed when RimCusine 1 or 2 is found ### Real World Food -->
<ThingDef ParentName="OrganicProductBase"> <!-- PlantFoodRawBase -->
    <defName>IAmHungry_Flour</defName>
    <label>flour</label>
    <description>Flour made from grinding any sort of grain. Used in baking and cooking, it is the staple of many societies, including here on the Rim.</description>
    <graphicData>
      <texPath>Things/Resource/Flour</texPath>
	  <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>2700</WorkToMake>
      <MarketValue>5</MarketValue>
	  <Mass>0.1</Mass>
      <Nutrition>0.05</Nutrition>
      <FoodPoisonChanceFixedHuman>0.1</FoodPoisonChanceFixedHuman>
    </statBases>
	<techLevel>Neolithic</techLevel>
    <ingestible>
      <foodType>Processed</foodType> <!-- Plant, Processed -->
	  <preferability>DesperateOnly</preferability>
      <!-- <preferability>RawBad</preferability> -->
	  <tasteThought>AteAwfulMeal</tasteThought>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>RawVegetable_Eat</ingestSound>
    </ingestible>
	<thingCategories Inherit="False">
      <li>PlantFoodRaw</li><!-- Patch RC1: GrainsProcessed -->
    </thingCategories>
	<comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>60</daysToRotStart>
		<rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <socialPropernessMatters>true</socialPropernessMatters> <!-- Veg -->
</ThingDef>

</Defs>