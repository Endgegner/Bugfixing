<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ########## Proper Food ########## -->
<ThingDef ParentName="MealBase">
    <defName>IAmHungry_Bread</defName>
    <label>crusty bread</label> <!-- loaf -->
    <description>Bakery's Finests, pleasantly scented and crunchy like no other. It has the wide favored form of a delicious baguette.\n\nThis gives you exactly the energy you need, for a long day of hard work.</description>
    <graphicData>
      <texPath>Things/Meal/BreadQuality</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <statBases>
         <DeteriorationRate>2</DeteriorationRate>
         <MarketValue>70</MarketValue>
         <WorkToMake>1600</WorkToMake>
      	 <Nutrition>1</Nutrition>
    </statBases>
    <ingestible>
    <preferability>MealFine</preferability>
		 <tasteThought>AteProperMeal_Strength</tasteThought>
         <ingestEffect>EatVegetarian</ingestEffect>
         <ingestSound>Meal_Eat</ingestSound>
        <outcomeDoers>
        <li Class="IngestionOutcomeDoer_GiveHediff">
          <hediffDef>Trunken_hediff_Bread</hediffDef>
          <severity>0.25</severity>
        </li>
		</outcomeDoers>
    </ingestible>
    <thingCategories Inherit="False">
      <li>ProperMeals</li>
    </thingCategories>
	<tradeability>Buyable</tradeability>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>6</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
</ThingDef>

